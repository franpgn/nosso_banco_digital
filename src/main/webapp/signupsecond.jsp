<head>
    <meta charset='utf-8'>
    <link href="https://fonts.googleapis.com/css2?family=Nunito:wght@600;700;800&display=swap" rel="stylesheet">
    <link rel='stylesheet' href="styles/pages/signupsecond.css">
    <title>Criando Conta</title>
</head>
<div id="page-signupsecond">
    <main>
        <div class="content-wrapper">
            <main>
                <h1>Nosso
                    Banco
                    Digital
                </h1>
            </main>
        </div>
        <form action="page" class="signupsecond-form">
            <fieldset>
                <div class="input-block">
                    <input type="text" placeholder="CEP" required name="cep">
                </div>
                <div class="input-block">
                    <input type="text" placeholder="Rua" required name="street">
                </div>
                <div class="input-block">
                    <input type="text" placeholder="Bairro" required name="neighborhood">
                </div>
                <div class="input-block">
                    <input type="text" placeholder="Complemento" required name="complement">
                </div>
                <div class="input-block">
                    <input type="text" placeholder="Cidade" required name="city">
                </div>
                <div class="input-block">
                    <input type="text" placeholder="Estado" required name="state">
                </div>
                <button class="confirm-button" type="submit">
                    Confirmar
                </button>
            </fieldset>
        </form>
    </main>
</div>